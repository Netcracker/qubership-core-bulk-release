<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d %-5p %thread %class{0} - %m%n</pattern>
        </encoder>
    </appender>
    <logger name="org.qubership.cloud.actions" level="DEBUG"/>
    <root level="ERROR">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>